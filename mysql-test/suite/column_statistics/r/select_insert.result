set global column_stats_control = ON;
INSERT INTO tbl2 (col2_1, col2_2)
SELECT col1_3, col1_4
FROM tbl1 WHERE col1_2 <= 100 AND col1_3 IN (1, 2, 3, 4, 5, 6);
