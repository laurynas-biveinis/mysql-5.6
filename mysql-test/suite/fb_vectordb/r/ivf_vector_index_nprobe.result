1010	[0.32, 0.32, 0, 0]	val1010	0.0008
1011	[0.28, 0.28, 0, 0]	val1011	0.0008
4	[0.004, 0.004, 0, 0]	val4	0.1752
3	[0.003, 0.003, 0, 0]	val3	0.1764
2	[0.002, 0.002, 0, 0]	val2	0.1776
1	[0.001, 0.001, 0, 0]	val1	0.1788
0	[0, 0, 0, 0]	val0	0.18
