CREATE TABLE t1 (
id BIGINT NOT NULL AUTO_INCREMENT PRIMARY KEY,
color varchar(20) not null,
vector1 JSON NOT NULL fb_vector_dimension 3,
INDEX vector_key_1(vector1)
FB_VECTOR_INDEX_TYPE 'flat'
);

