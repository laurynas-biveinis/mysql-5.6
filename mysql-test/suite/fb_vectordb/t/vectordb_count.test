CREATE TABLE t1 (
  id BIGINT NOT NULL PRIMARY KEY,
  a int,
  b int,
  vector1 JSON NOT NULL fb_vector_dimension 3,
  INDEX vector_key_1(vector1) FB_VECTOR_INDEX_TYPE 'flat'
);

